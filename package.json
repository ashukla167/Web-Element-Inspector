    "build:dev-watch": "cross-env NODE_ENV=development webpack --progress --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "yarn cache clean&&npm cache clean",
    "preinstall:ext": "npm install cross-env",
    "install:ext": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 yarn install"
  },
